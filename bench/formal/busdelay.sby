[tasks]
prf
prfd prf opt_delay_stall

[options]
prf: mode prove
depth 14

[engines]
smtbmc

[script]
read -formal -D BUSDELAY busdelay.v
read -formal -D BUSDELAY fwb_slave.v
read -formal -D BUSDELAY fwb_master.v
opt_delay_stall:  hierarchy -top busdelay -chparam opt_delay_stall 1
~opt_delay_stall: hierarchy -top busdelay -chparam opt_delay_stall 0
prep -top busdelay

[files]
../../rtl/ex/busdelay.v
../../rtl/ex/fwb_slave.v
../../rtl/ex/fwb_master.v
